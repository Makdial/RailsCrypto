<h1>Crypto App</h1>

<br><br>

<% @coins %>

<% for x in @coins %>
    <% for coin in @my_coins %>
        <% if coin == x["symbol"] %>
            <%= x["name"] %>: <strong> $<%= x["price_usd"]%><br/></strong>
            Rank : <%= x["rank"]%><br/><br/>
        <% end %> 
    <% end %>
<% end %>

